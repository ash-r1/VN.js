{
    "name": "vnjs",
    "displayName": "vnjs",
    "description": "Syntax Highlighting for vnjs-script (.vs)",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "vnjs-script",
            "aliases": ["VNJS", "vnjs-script"],
            "extensions": [".vs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vnjs-script",
            "scopeName": "source.vnjs",
            "path": "./syntaxes/vnjs-script.tmLanguage.json"
        }]
    }
}